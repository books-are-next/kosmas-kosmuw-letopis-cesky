        <navPoint id="navPoint-7" playOrder="7">
            <navLabel>
                <text>Počíná se předmluwa téhož děkana swrchuřečeného k třetím knihám téhož díla.</text>
            </navLabel>
            <content src="Text/9.xhtml"/>
        </navPoint>
        <navPoint id="navPoint-8" playOrder="8">
            <navLabel>
                <text>Počínají se knihy třetí téhož díla.</text>
